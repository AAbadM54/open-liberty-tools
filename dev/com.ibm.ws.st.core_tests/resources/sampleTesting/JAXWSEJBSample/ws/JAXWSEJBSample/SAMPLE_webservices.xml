<webservices version="1.3" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/javaee_web_services_1_3.xsd ">
  <webservice-description>
    <description>description</description>
    <display-name>displayName</display-name>
    <icon>
      <small-icon>icon</small-icon>
      <large-icon>icon</large-icon>
    </icon>
    <webservice-description-name>webserviceDescriptionName</webservice-description-name>
    <wsdl-file>wsdlLocation</wsdl-file>
    <jaxrpc-mapping-file>jaxRpcMappingFile</jaxrpc-mapping-file>
    <port-component>
      <description>description</description>
      <display-name>displayName</display-name>
      <icon>
        <small-icon>icon</small-icon>
        <large-icon>icon</large-icon>
      </icon>
      <port-component-name>portTypeName</port-component-name>
      <wsdl-service>QName</wsdl-service>
      <wsdl-port>QName</wsdl-port>
      <enable-mtom>true</enable-mtom>
      <mtom-threshold>0</mtom-threshold>
      <addressing>
        <enabled>true</enabled>
        <required>true</required>
        <responses>ANONYMOUS</responses>
      </addressing>
      <respect-binding>
        <enabled>true</enabled>
      </respect-binding>
      <protocol-binding>bindingType or token</protocol-binding>
      <service-endpoint-interface>fullyQualifiedClassName</service-endpoint-interface>
      <service-impl-bean>
        <ejb-link>ejbName</ejb-link>   
        <!--<servlet-link>servletName if defined in web.xml or fullyQualifiedClassName for POJO webservices</servlet-link>-->
      </service-impl-bean>
      <handler>
        <handler-name>handlerName</handler-name>
        <handler-class>fullyQualifiedClassName</handler-class>
        <init-param>
          <description>description</description>
          <param-name>paramName</param-name>
          <param-value>paramValue</param-value>
        </init-param>
        <soap-header>QName</soap-header>
        <soap-role>soapRole</soap-role>
        <port-name>portName</port-name>
      </handler>
    </port-component>
  </webservice-description>
</webservices>
